<!DOCTYPE html>
<html data-ng-app="AngularAuthApp">
<head>
    <title>Test</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/loading-bar.min.css" rel="stylesheet"/>
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
</head>
<body>
    <div id="preloader" class="preloader">
        <div class="loading">
            <h2>Loading...</h2>
            <div class="progress"></div>
        </div>
    </div>
    <div class="page-bg">
        <div class="page-bg__smoke">
            <div class="page-bg__smoke-img1">
                <img alt="smoke-image-1" src="img/smoke1.png">
            </div>
            <div class="page-bg__smoke-img2">
                <img alt="smoke-image-2" src="img/smoke2.png">
            </div>
            <div class="page-bg__smoke-img3">
                <img alt="smoke-image-3" src="img/smoke3.png">
            </div>
        </div>
    </div>
    <div class="page">
        <div class="logo-wrap">
            <div class="logo-wrap__i"><a href="#" class="logo"><img alt="logo" src="img/logo.png"></a></div>
        </div>
        <div class="page__slider">
            <div class="page__slider-content">
                <div class="page__slider-content-inner">
                    <div id="start" class="start">
                        <div ng-app="whr-countdown-app" class="row">
                            <div id="countdown" countdown="" date="January 1, 2016 00:00:00"></div>
                        </div>
                        <p class="sub-title">Help your friends to choose gift for You!</p>
                        <button id="startButton" class="button">start</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="progressBar" class="progress-bar">
        <div class="progress-bar__inner"></div>
    </div>
    <footer><a href="#">&copy; Strong Soft Team</a></footer>
    <button id="logOut" class="button log-out hide">Log out</button>
 


    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation" data-ng-controller="indexController">
        <div class="container">
            <div class="navbar-header">
                <button class="btn btn-success navbar-toggle" data-ng-click="navbarExpanded = !navbarExpanded">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </button>
                <a class="navbar-brand" href="#/">Home</a>
            </div>
            <div class="collapse navbar-collapse" data-collapse="!navbarExpanded">
                <ul class="nav navbar-nav navbar-right">
                    <li data-ng-hide="!authentication.isAuth"><a href="#">Welcome {{authentication.userName}}</a></li>
                    <li data-ng-hide="!authentication.isAuth"><a href="" data-ng-click="logOut()">Logout</a></li>
                    <li data-ng-hide="authentication.isAuth"> <a href="#/login">Login</a></li>
                    <li data-ng-hide="authentication.isAuth"> <a href="#/signup">Sign Up</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="jumbotron">
        <div class="container">
            <div class="page-header text-center">
                <h1>yo login pls</h1>
            </div>
        </div>
    </div>

    <div class="container">
        <div data-ng-view="">
        </div>
    </div>

    <hr />


    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="../../Scripts/angular-local-storage.min.js"></script>
    <script src="../../Scripts/loading-bar.min.js"></script>


    <script src="~/ClientJS/app.js?v=2"></script>
    <!-- Load services -->
    <script src="~/ClientJS/services/authInterceptorService.js?v=2"></script>
    <script src="~/ClientJS/services/authService.js?v=2"></script>
    <script src="~/ClientJS/services/tokensManagerService.js?v=2"></script>
    <!-- Load controllers -->

    <script src="~/ClientJS/controllers/indexController.js?v=2"></script>
    <script src="~/ClientJS/controllers/homeController.js?v=2"></script>
    <script src="~/ClientJS/controllers/loginController.js?v=2"></script>
    <script src="~/ClientJS/controllers/signupController.js?v=2"></script>
    <script src="~/ClientJS/controllers/associateController.js?v=2"></script>
    <!-- Load custom filters -->
    <!-- Load custom directives -->

</body>
</html>